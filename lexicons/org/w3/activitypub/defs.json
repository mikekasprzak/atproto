          "knownValues": [
            "video/x-msvideo",
            "video/mp4",
            "video/mpeg",
            "video/ogg",
            "video/mp2t",
            "video/webm",
            "video/3gpp",
            "video/3gpp2"
          ]
        }
      }
    },
    "sourceType" : {
      "type": "object",
      "properties": {
        "content" : { "type": "string" },
        "mediaType": { "type": "ref", "ref": "#textMimeType" }
      }
    },
    "urlObjectType" : {
      "type": "object",
      "properties": {
        "type": { "type": "ref", "ref": "#objectType" },
        "href" : { "type": "string", "format": "uri" },
        "mediaType": { "type": "ref", "ref": "#mimeType" }
      }
    }
  }
}
